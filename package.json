    "postcss": "^8.2.6",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.1.4",
    "rollup": "^2.39.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
